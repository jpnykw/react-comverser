{
  "name": "react-comverse",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "star": "tsc && node ./dist/index.js",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts}\"",
    "test": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,sass,scss}": [
      "stylelint --fix"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@types/acorn": "^4.0.6",
    "@types/escodegen": "^0.0.7",
    "@types/node": "^16.11.6",
    "@types/prettier": "^2.4.1",
    "acorn": "^8.5.0",
    "acorn-jsx": "^5.3.2",
    "escodegen": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@zoltu/typescript-transformer-append-js-extension": "^1.0.1",
    "eslint": "^8.2.0",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "sirv-cli": "^1.0.14",
    "ts-jest": "^27.0.7",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.4",
    "yarn-run-all": "^3.1.1"
  }
}
